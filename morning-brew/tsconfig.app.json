{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "inlineSources": true,
    "sourceMap": true
  },
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}